
<div class="container mb-3">
 <div class="row justify-content-center">
    <h1>Lists</h1>
    <p>
      <% @lists.each do |list| %>
        <%= link_to list.name, list_path(list) %>
      <% end %>
    </p>

    <%= link_to "Add a new list", new_list_path, class: "btn btn-primary" %>
  </div>
</div>
